  "files": [
    "dist/**",
    "package.json"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/app-dir.ts --format esm,cjs --dts",
    "dev": "tsc --watch"
  },
  "peerDependencies": {
    "next": "13.2.4",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@swc/core": "1.3.42",
    "@types/node": "18.15.10",
    "@types/react": "18.0.29",
    "@types/react-dom": "18.0.11",
    "next": "13.2.4",
    "prettier": "^2.8.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tsup": "6.7.0",
    "typescript": "5.0.2"
  },
  "packageManager": "yarn@3.5.0"
}
